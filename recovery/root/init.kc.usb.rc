# QFunctionMode
on property:sys.usb.config=diag,serial_smd,rmnet_qti_bam,adb
    stop adbd
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 05C6
    write /sys/class/android_usb/android0/idProduct 9091
    write /sys/class/android_usb/android0/f_diag/clients diag
    write /sys/class/android_usb/android0/f_serial/transports smd
    write /sys/class/android_usb/android0/f_rmnet/transports qti,bam
    write /sys/class/android_usb/android0/functions diag,serial,rmnet,adb
    write /sys/class/android_usb/android0/enable 1
    start adbd
    setprop sys.usb.state ${sys.usb.config}

##########################################################################################
on property:sys.usb.tethering=true
    write /sys/class/net/usb0/queues/rx-0/rps_cpus ${sys.usb.rps_mask}

on property:vold.decrypt=trigger_post_fs_data
    start usb_init

##########################################################################################
on property:sys.usb.kc_debug.log=on
    write /sys/module/phy_msm_usb/parameters/phy_msm_usb_debug_log 1
    write /sys/module/ci13xxx_msm/parameters/ci13xxx_debug_log 1
    write /sys/module/g_android/parameters/android_debug_log 1

on property:sys.usb.kc_debug.log=off
    write /sys/module/phy_msm_usb/parameters/phy_msm_usb_debug_log 0
    write /sys/module/ci13xxx_msm/parameters/ci13xxx_debug_log 0
    write /sys/module/g_android/parameters/android_debug_log 0
