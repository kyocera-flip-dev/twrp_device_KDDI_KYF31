
on init
    write /sys/class/android_usb/android0/f_rndis/wceis 1
    write /sys/class/android_usb/android0/iManufacturer "KYOCERA"
    write /sys/class/android_usb/android0/iProduct "KYOCERA_Android"
    write /sys/class/android_usb/android0/f_mass_storage/inquiry_string "KYOCERA MSS             0100"

on fs
    mkdir /dev/usb-ffs 0770 shell shell
    mkdir /dev/usb-ffs/adb 0770 shell shell
    mount functionfs adb /dev/usb-ffs/adb uid=2000,gid=2000
    write /sys/class/android_usb/android0/f_ffs/aliases adb

service kc-usb-sh /system/bin/sh /init.kc.usb.sh
    class core
    user root
    oneshot

# USB compositions
on property:sys.usb.config=mass_storage,diag
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 0482
    write /sys/class/android_usb/android0/idProduct 0a7f
    write /sys/class/android_usb/android0/f_diag/clients diag
    write /sys/class/android_usb/android0/functions diag,mass_storage
    write /sys/class/android_usb/android0/enable 1
    setprop sys.usb.state ${sys.usb.config}

on property:sys.usb.config=mass_storage
    write /sys/class/android_usb/android0/enable 0
    write /sys/class/android_usb/android0/idVendor 0482
